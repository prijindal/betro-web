{
  "name": "betro-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@betro/client": "0.0.3",
    "@heroicons/react": "^1.0.6",
    "@mantine/core": "^3.6.14",
    "@mantine/hooks": "^3.6.14",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.26.1",
    "eslint-config-react-app": "^7.0.0",
    "immer": "^9.0.14",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-infinite-scroll-hook": "^4.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "sanitize-html": "^2.7.0",
    "sass": "^1.49.9",
    "source-map-explorer": "^2.5.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "vite",
    "build": "tsc && vite build",
    "build:docker": "docker build -t betro/web .",
    "docker:publish": "docker push betro/web:latest",
    "build-with-stats": "npm run build && npm run analyze",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude \"node_modules/*\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "lint": "concurrently \"yarn-or-npm run lint:js\" \"yarn-or-npm run lint:css\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:css": "stylelint src/**/*.css src/**/*.scss",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "eject": "react-scripts eject",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "browserslist": "^4.20.0",
    "compression-webpack-plugin": "^9.2.0",
    "concurrently": "^7.0.0",
    "depcheck": "^1.4.3",
    "dependency-cruiser": "^11.4.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "jest-puppeteer": "^6.1.0",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "puppeteer": "^13.5.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "scss": "^0.2.4",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.2",
    "vite": "^2.8.6",
    "yarn-or-npm": "^3.0.1"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001228"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
