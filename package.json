{
  "name": "betro-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.0.0-alpha.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@heroicons/react": "^1.0.5",
    "@material-ui/core": "^5.0.0-beta.5",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.1",
    "axios": "^0.25.0",
    "betro-js-client": "0.0.1",
    "history": "^5.2.0",
    "i18next": "^21.6.11",
    "i18next-xhr-backend": "^3.2.2",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "react-i18next": "^11.15.4",
    "react-infinite-scroll-hook": "^4.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.0-beta.8",
    "react-router-dom": "^6.0.0-beta.8",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "sanitize-html": "^2.7.0",
    "sass": "^1.49.7",
    "source-map-explorer": "^2.5.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "build": "craco build",
    "build:docker": "docker build -t betro/web .",
    "docker:publish": "docker push betro/web:latest",
    "build-with-stats": "npm run build && npm run analyze",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude \"node_modules/*\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "lint": "concurrently \"yarn-or-npm run lint:js\" \"yarn-or-npm run lint:css\"",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:css": "stylelint src/**/*.css src/**/*.scss",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "test": "craco test",
    "eject": "react-scripts eject",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.4.19",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/marked": "^3.0.2",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/sanitize-html": "^2.6.2",
    "autoprefixer": "^10.4.2",
    "browserslist": "^4.19.1",
    "compression-webpack-plugin": "^9.2.0",
    "concurrently": "^7.0.0",
    "depcheck": "^1.4.3",
    "dependency-cruiser": "^11.3.1",
    "husky": "^7.0.4",
    "jest-puppeteer": "^6.1.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "puppeteer": "^13.3.1",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "3.0.22",
    "typescript": "4.5.5",
    "yarn-or-npm": "^3.0.1"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001228"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
